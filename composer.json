{
    "name": "multek/laravel-geoaddress",
    "description": "Production-ready polymorphic address system with automatic geocoding for Laravel",
    "keywords": ["laravel", "address", "geocoding", "postgis", "polymorphic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Multek",
            "email": "dev@multek.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "matanyadaev/laravel-eloquent-spatial": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "spatie/geocoder": "^3.0"
    },
    "suggest": {
        "spatie/geocoder": "Required for Google Maps geocoding provider"
    },
    "autoload": {
        "psr-4": {
            "Multek\\LaravelGeoaddress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Multek\\LaravelGeoaddress\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Multek\\LaravelGeoaddress\\GeoaddressServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
